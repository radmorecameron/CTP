@{
    ViewData["Title"] = "Release Notes";
}

<h2>Release Notes</h2>


<div class="accordion" id="accordionRelease">
    <div class="card">
        <div class="card-header" id="headingSeven">
            <h2 class="mb-0">
                <button class="btn btn-light" type="button" data-bs-toggle="collapse" href="#collapseSeven" aria-expanded="true" aria-controls="collapseSeven">
                    Version 0.1 - March 28th, 2022
                </button>
            </h2>
        </div>
        <div id="collapseSeven" class="collapse" aria-labelledby="headingSix" data-parent="#accordionRelease">
            <div class="card-body">
                <p>What's New?</p>
                <ul>
                    <li>
                        Website
                        <ul>
                            <li>Website created.</li>
                            <li>Login page has been created.</li>
                            <li>Teacher and Student homepage created.</li>
                        </ul>
                    </li>
                    <li>
                        Teacher
                        <ul>
                            <li>Can create,update, view and delete Test Cases, Method Signatures and Activities.</li>
                            <li>Can invite selected and next student to the table.</li>
                            <li>Can remove students from the their table.</li>
                        </ul>
                    </li>
                    <li>
                        Student
                        <ul>
                            <li>Can upload code and have their code ran against the test cases.</li>
                        </ul>
                    </li>
                </ul>
            </div>
        </div>
    </div>
</div>