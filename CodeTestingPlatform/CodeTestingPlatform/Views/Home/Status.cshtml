@model ResponseStatus
@{
    ViewBag.Title = "Page Not Found";
}
<div id="notfound">
    <div class="notfound">
        <div class="notfound-404">
            <h1>Oops!</h1>
        </div>
        <h2>@Model.Code - @Model.Title</h2>
        <p>@Model.Description</p>
        <a href="~/">Go To Homepage</a>
    </div>
</div>
@section Styles{
    <link rel="stylesheet" href="~/css/shared/error.css">
}